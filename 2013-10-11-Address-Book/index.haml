%head
  %meta{charset: "utf-8"}
  %meta{name: "viewport", content: "width=device-width"}
  %title Rachel's Address Book
  %link{rel: "stylesheet", href: "css/normalize.css"}
  %link{rel: "stylesheet", href: "css/foundation.css"}
  %link{rel: "stylesheet", href: "css/app.css"}
  %script{src: "js/vendor/custom.modernizr.js"}
%body
  .row
    .small-12.column
      %h1 Address Book App
  .row
    .small-3.column
      %input#name{type: 'text', placeholder: 'name'}
    .small-7.column
      %input#address{type: 'text', placeholder: 'address'}
    .small-2.column
  .row
    .small-3.column
      %input#website{type: 'text', placeholder: 'website'}
    .small-3.column
      %input#email{type: 'text', placeholder: 'email'}
    .small-3.column
      %input#photoUrl{type: 'text', placeholder: 'url'}
    .small-3.column
      %input#addPersonBtn.button.small.success.radius{type: 'button', value: 'add Person'}
  .row
    .small-12.column
      #listing




  %script{src: 'js/vendor/jquery-1.10.2.js'}
  %script{src: 'js/vendor/foundation.min.js'}
  %script{src: 'js/vendor/underscore.js'}
  %script{src: 'js/vendor/moment.min.js'}
  %script{src: 'https://cdn.firebase.com/v0/firebase.js'}
  %script{src: 'js/app/app.js'}
